<div class="alert alert-info">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <h5><i class="icon-calendar"> </i> Calendar</h5>
    <ul>
    {% for event in eventList %}
        <li>{{ event.title }} {{ event.location }} {{ event.date|date('d/m') }}</li>
        {# render "ChouffeMagicBundle:Event:see" with {'id':event.id} #}
    {% else %}
        No event planned...
    {% endfor %}
    </ul>
</div>
